@misc{TaggenPublikationen,
  title = {Taggen von {{Publikationen}}},
  abstract = {SIEHE AUCH: how-to\_gp-bibliographie Alle Publikationen, die in die GESIS Panel Bibliographie aufgenommen werden, erhalten je nach Eigenschaften in Zotero verschiedene Tags - Alle Einträge erhalten das Tag -{$>$} „GESISpanel“ - Journal-Beiträge:     * Begutachtete Aufsätze in Zeitschriften -{$>$} „article\_reviewed“     * Aufsätze ohne Begutachtung in Zeitschriften (inkl. Zeitungen) -{$>$} „not\_reviewed“ - Arbeits- und Diskussionspapiere -{$>$} „techreport“ - Einzelbeiträge in Sammelwerken (inkl. Lehrbüchern) -{$>$} „incollection“ - Qualifikationsarbeiten -{$>$} je nach Art entweder „phdthesis“ oder „masterthesis“, „bachelorthesis“ - Im Erscheinen -{$>$} „unpublished“  - Beiträge die unsere Daten nur beschreiben, aber nicht analysieren -{$>$} „description“ - Beiträge in dem Ordner: 02\_gp\_bib\_non\_public -{$>$} „gp\_bib\_non\_public“ - Beiträge, die Daten der Pilot-Studie nutzen -{$>$} „gp\_gopp“ - Beiträge, die im GESIS report beschrieben wurden -{$>$} „GESISreport“ - Alle Einträge die erkennbar in absehbarer Zeit aktualisiert werden erhalten den Tag „item\_revision“. Diese Publikationen müssen regelmäßig auf Aktualität geprüft und ggf. angepasst werden.}
}

@report{thunGermanCoronaConsensus2020,
  title = {The {{German Corona Consensus Dataset}} ({{GECCO}}): {{A}} Standardized Dataset for {{COVID}}-19 Research},
  shorttitle = {The {{German Corona Consensus Dataset}} ({{GECCO}})},
  author = {Thun, Sylvia and Sass, Julian and Bartschke, Alexander and Lehne, Moritz and Essenwanger, Andrea and Rinaldi, Eugenia and Rudolph, Stefanie and Heitmann, Kai U. and Vehreschild, Jörg J. and von Kalle, Christof},
  date = {2020-08-18},
  institution = {{In Review}},
  doi = {10.21203/rs.3.rs-51348/v1},
  url = {https://www.researchsquare.com/article/rs-51348/v1},
  urldate = {2020-09-28},
  abstract = {Abstract                        Background:             The current COVID-19 pandemic has led to a surge of research activity. While this research provides important insights, the multitude of studies results in an increasing segmentation of information. To ensure comparability across projects and institutions, standard datasets are needed. Here, we introduce the “German Corona Consensus Dataset” (GECCO), a uniform dataset that uses international terminologies and health IT standards to improve interoperability of COVID-19 data.             Methods:             Based on previous work (e.g., the ISARIC-WHO COVID-19 case report form) and in coordination with experts from university hospitals, professional associations and research initiatives, data elements relevant for COVID-19 research were collected, prioritized and consolidated into a compact core dataset. The dataset was mapped to international terminologies, and the Fast Healthcare Interoperability Resources (FHIR) standard was used to define interoperable, machine-readable data formats.             Results:             A core dataset consisting of 81 data elements with 281 response options was defined, including information about, for example, demography, anamnesis, symptoms, therapy, medications or laboratory values of COVID-19 patients. Data elements and response options were mapped to SNOMED CT, LOINC, UCUM, ICD-10-GM and ATC, and FHIR profiles for interoperable data exchange were defined.             Conclusion:             GECCO provides a compact, interoperable dataset that can help to make COVID-19 research data more comparable across studies and institutions. The dataset will be further refined in the future by adding domain-specific extension modules for more specialized use cases.},
  file = {E\:\\references\\storage\\JZD8PZ43\\Thun et al. - 2020 - The German Corona Consensus Dataset (GECCO) A sta.pdf},
  keywords = {GESISpanel},
  type = {preprint}
}

