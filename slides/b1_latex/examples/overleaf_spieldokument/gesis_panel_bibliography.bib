@unpublished{minderopUsingResponsePatterns2019,
  title = {Using response patterns in a panel survey to explain panel dropout},
  author = {Minderop, Isabella and Weiß, Bernd},
  date = {2019-07-18},
  url = {https://www.europeansurveyresearch.org/conf2019/uploads/421/3003/104/esra19_minderop.pdf},
  urldate = {2019-10-30},
  abstract = {Keeping respondents who are likely to drop out of a panel in the sample is a major task for panel data infrastructures. This is especially important when respondents at risk of dropping out are notably different to other respondents. Hence, it is key to identify those respondents and prevent them from dropping out. Response behavior in previous waves, e.g. response or nonresponse, has been shown to be a good predictor of next wave's response. Studying patterns of time-to-response and nonresponse over multiple panel waves can be a promising approach to identify participants with a high likelihood of leaving the panel.},
  keywords = {GESISpanel,presentations_paper},
  langid = {Englisch},
  venue = {{ESRA Conference}}
}

