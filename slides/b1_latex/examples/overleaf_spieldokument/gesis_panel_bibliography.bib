@article{rummelShortVersionMetacognitive2019,
  title = {The Short Version of the {{Metacognitive Prospective Memory Inventory}} ({{MPMI}}-s): Factor Structure, Reliability, Validity, and Reference Data},
  shorttitle = {The Short Version of the {{Metacognitive Prospective Memory Inventory}} ({{MPMI}}-s)},
  author = {Rummel, Jan and Danner, Daniel and Kuhlmann, Beatrice G.},
  date = {2019-06-21},
  journaltitle = {Measurement Instruments for the Social Sciences},
  shortjournal = {Measurement Instruments for the Social Sciences},
  volume = {1},
  pages = {1},
  issn = {2523-8930},
  doi = {10.1186/s42409-019-0008-6},
  url = {https://doi.org/10.1186/s42409-019-0008-6},
  urldate = {2019-07-01},
  abstract = {Prospective memory, the ability to remember to execute an intended action at the appropriate moment in the future, is frequently assessed with standardized questionnaires. Prospective-memory abilities strongly depend on the different strategies people use to remember their intentions. In this study, we introduce the short version of the Metacognitive Prospective Memory Inventory (MPMI-s) that allows for a quick assessment of individual differences in self-reported prospective-memory abilities as well as in the use of mnemonic strategies (e.g., intention rehearsal) and external memory aids (e.g., calendars). Based on data from two waves of the GESIS longitudinal panel, we provide evidence that this novel questionnaire offers reliable and valid measures of prospective-memory abilities as well as of internal and external strategy use. As the panel sample is representative of the German population, we are further able to provide reference data that allow evaluating individual PM ability and strategy-use scores obtained with the MPMI-s.},
  file = {E\:\\references\\storage\\6GMN89UI\\s42409-019-0008-6.html},
  keywords = {2020_report,article_reviewed,bibsonomy,GESISpanel},
  number = {1}
}

