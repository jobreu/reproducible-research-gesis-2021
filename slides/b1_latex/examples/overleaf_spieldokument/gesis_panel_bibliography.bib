@article{gummerEarlyLateParticipation2020,
  title = {Early and {{Late Participation}} during the {{Field Period}}: {{Response Timing}} in a {{Mixed}}-{{Mode Probability}}-{{Based Panel Survey}}},
  shorttitle = {Early and {{Late Participation}} during the {{Field Period}}},
  author = {Gummer, Tobias and Struminskaya, Bella},
  date = {2020-04-22},
  journaltitle = {Sociological Methods \& Research},
